import pandas as pd

# Sample DataFrame for illustration
data = {
    'c1': ['A', 'B', 'A', 'C', 'B', 'D', 'A', 'C', 'D', 'D', 'E', 'F', 'G', 'H', 'I', 'J']
}

# Create the original DataFrame
df4 = pd.DataFrame(data)

# Remove duplicates from the 'c1' column
df_unique = df4.drop_duplicates(subset='c1').reset_index(drop=True)

print(df_unique)
