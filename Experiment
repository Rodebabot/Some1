import pandas as pd

# Assuming df_existing is your existing DataFrame
# Assuming df_new is your new DataFrame

# Find rows that are in df_new but not in df_existing
new_rows = pd.concat([df_new, df_existing]).drop_duplicates(keep=False)

# Append only the new rows to df_existing
df_existing = pd.concat([df_existing, new_rows], ignore_index=True)
