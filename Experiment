import pandas as pd

# Assuming you have a dataframe df with columns 'team' and 'capacity'

# Sample data
# df = pd.read_excel("path_to_your_excel_file.xlsx")

# Extracting required columns and creating the new column
new_df = df[['team', 'capacity']].copy()
new_df['new_capacity'] = df['capacity'].apply(lambda x: x if x > 0 else 2400)

print(new_df)
