import pandas as pd

def reorder_columns(df, desired_order):
  """
  Reorders columns in a dataframe to match a desired order.

  Args:
      df (pandas.DataFrame): The dataframe to reorder.
      desired_order (list): A list containing the desired order of columns.

  Returns:
      pandas.DataFrame: The dataframe with reordered columns.
  """
  existing_columns = set(df.columns)
  reordered_columns = desired_order[: len(existing_columns)]  # Limit to existing columns
  remaining_columns = [col for col in df.columns if col not in reordered_columns]
  return df[reordered_columns + remaining_columns]

# Assuming your dataframe is named 'df'
desired_order = ["Entity", "cpty", "Deb", "rib"]
df_reordered = reorder_columns(df.copy(), desired_order)

# Print the dataframe with reordered columns (optional)
print(df_reordered)
