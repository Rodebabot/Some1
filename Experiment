Sub GenerateRandomNumbersAndMultiply()
    Dim rng As Range
    Dim cell As Range
    
    ' Set the range for the first column (A1:A15)
    Set rng = Range("A1:A15")
    
    ' Generate random numbers for the first column
    For Each cell In rng
        cell.Value = Int((100 - 1 + 1) * Rnd + 1) ' Generate random numbers between 1 and 100
    Next cell
    
    ' Multiply each number in the first column by 2 and put the result in the second column (B1:B15)
    For Each cell In rng
        cell.Offset(0, 1).Value = cell.Value * 2
    Next cell
End Sub
