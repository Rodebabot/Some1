import pandas as pd

# Sample data
data = {'Name': ['John', 'Alice', 'John', 'Bob', 'Alice'],
        'String': ['String1', 'String2', 'String3', 'String4', 'String5']}

# Create a DataFrame
df = pd.DataFrame(data)

# Group by the 'Name' column and join the 'String' column values with a comma
result = df.groupby('Name')['String'].apply(lambda x: ', '.join(x)).reset_index()

print(result)
