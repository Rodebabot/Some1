# Assuming df is your DataFrame
# And you have installed the openpyxl library which is required for writing XLSB files

# Specify the file path for the XLSB file
file_path = "output.xlsb"

# Write the DataFrame to the XLSB file with sheet name "breaks"
with pd.ExcelWriter(file_path, engine='pyxlsb') as writer:
    df.to_excel(writer, sheet_name='breaks', index=False)
