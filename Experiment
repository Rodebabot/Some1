import pandas as pd

# Assuming your DataFrame is named df
# Calculate the bin size
max_value = df['Final Weighted Score'].max()
bin_size = max_value / 5

# Define the bins starting from 0
bins = [i * bin_size for i in range(6)]

# Define the labels for the groups
labels = ['Group A', 'Group B', 'Group C', 'Group D', 'Group E']

# Create a new column with the group labels based on the bins
df['Group'] = pd.cut(df['Final Weighted Score'], bins=bins, labels=labels, include_lowest=True)

# Display the DataFrame with the new 'Group' column
print(df)
