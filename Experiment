Sub ConcatenateColumns()
    Dim wb As Workbook
    Dim ws As Worksheet
    Dim lastRow As Long
    Dim i As Long
    
    ' Open the "3rd" Excel file
    Set wb = Workbooks.Open("C:\path\to\your\3rd.xlsx") ' Update the file path as needed
    
    ' Set the worksheet you want to work with
    Set ws = wb.Sheets(1) ' Assuming the data is on the first sheet
    
    ' Find the last row with data in column A (assuming data starts from A1)
    lastRow = ws.Cells(ws.Rows.Count, "A").End(xlUp).Row
    
    ' Concatenate values from columns A and B and put the result in column C
    For i = 1 To lastRow
        ws.Cells(i, 3).Value = ws.Cells(i, 1).Value & " " & ws.Cells(i, 2).Value
    Next i
    
    ' Rename the third column to "C"
    ws.Cells(1, 3).Value = "C"
    
    ' Save and close the workbook
    wb.Close SaveChanges:=True ' Change to False if you don't want to save changes
End Sub
