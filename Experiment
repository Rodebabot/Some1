import pandas as pd

# Assuming dfv is your bigger DataFrame
# Assuming 'Value Date' column is in datetime format

# Boolean indexing to filter rows with 'Value Date' in October, November, or December of 2023
filtered_df = dfv[(dfv['Value Date'].dt.year == 2023) & 
                 (dfv['Value Date'].dt.month.isin([10, 11, 12]))]

# Now filtered_df contains rows with 'Value Date' in October, November, or December of 2023
