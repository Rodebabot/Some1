import tkinter as tk
from tkinter import ttk

def function1():
    print("Function 1")

def function2():
    print("Function 2")

def combined_functions():
    function1()
    function2()

# Create the main Tkinter window
root = tk.Tk()
root.title("Multiple Function Trigger")

# Create a button that triggers multiple functions
button = ttk.Button(root, text="Trigger Functions", command=combined_functions)
button.pack(padx=20, pady=20)

root.mainloop()
