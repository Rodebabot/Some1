import pandas as pd

# Load the Excel file into a pandas DataFrame
df = pd.read_excel("your_excel_file.xlsx")

# Find all indices where 'Original TRN' is present in the first column
original_indices = df.index[df.iloc[:, 0] == 'Original TRN']

# Iterate through each occurrence of 'Original TRN' and delete the rows, except the first occurrence
for original_index in original_indices[1:]:
    df.drop(original_index, inplace=True)

# Save the modified DataFrame back to Excel
df.to_excel("modified_excel_file.xlsx", index=False)
