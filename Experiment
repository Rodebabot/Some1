import dash
from dash import dcc, html
from dash_table import DataTable
import pandas as pd

# Assuming you already have dfa, dfb, and dfc dataframes

# Create Dash app
app = dash.Dash(__name__)

# Layout
app.layout = html.Div([
    html.H2('DataFrame A'),
    DataTable(
        id='datatable-a',
        columns=[{"name": i, "id": i} for i in dfa.columns],
        data=dfa.to_dict('records'),
    ),
    html.H2('DataFrame B'),
    DataTable(
        id='datatable-b',
        columns=[{"name": i, "id": i} for i in dfb.columns],
        data=dfb.to_dict('records'),
    ),
    html.H2('DataFrame C'),
    DataTable(
        id='datatable-c',
        columns=[{"name": i, "id": i} for i in dfc.columns],
        data=dfc.to_dict('records'),
    ),
])

if __name__ == '__main__':
    app.run_server(debug=True)
