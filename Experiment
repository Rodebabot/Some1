import pandas as pd

# Example DataFrame
data = {'A': [1, 2, 3],
        'B': [4, 5, 6],
        'C': [7, 8, 9],
        'D': [10, 11, 12]}
df = pd.DataFrame(data)

# Check if DataFrame has at least two columns
if len(df.columns) >= 2:
    # Delete the penultimate column
    df.drop(df.columns[-2], axis=1, inplace=True)
else:
    print("DataFrame has less than two columns, cannot delete the penultimate column.")

print(df)
