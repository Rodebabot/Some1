import pandas as pd

# Read the Excel file
excel_file = 'your_file.xlsx'
xls = pd.ExcelFile(excel_file)

# Create empty dataframes
df1 = pd.DataFrame()
df2 = pd.DataFrame()
df3 = pd.DataFrame()

# Iterate over each sheet
for sheet_name in xls.sheet_names:
    # Read the sheet skipping the first 3 rows
    df_temp = pd.read_excel(excel_file, sheet_name=sheet_name, skiprows=3)
    
    # Assign dataframes based on sheet index
    if xls.sheet_names.index(sheet_name) == 0:
        df1 = df_temp
    elif xls.sheet_names.index(sheet_name) == 1:
        df2 = df_temp
    elif xls.sheet_names.index(sheet_name) == 2:
        df3 = df_temp

# Output the dataframes
print("DataFrame 1:")
print(df1)
print("\nDataFrame 2:")
print(df2)
print("\nDataFrame 3:")
print(df3)
