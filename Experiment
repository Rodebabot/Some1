import dash
from dash import html
from dash import dcc
import dash_table

# Assuming you have existing dataframes: dfa, dfab, and dfc
# Replace the sample dataframes with your actual dataframes

# Initialize Dash app
app = dash.Dash(__name__)

# Define Dash layout
app.layout = html.Div([
    html.H1("Dataframe A"),
    dash_table.DataTable(
        id='table-dfa',
        columns=[{"name": i, "id": i} for i in dfa.columns],
        data=dfa.to_dict('records')
    ),

    html.H1("Dataframe AB"),
    dash_table.DataTable(
        id='table-dfab',
        columns=[{"name": i, "id": i} for i in dfab.columns],
        data=dfab.to_dict('records')
    ),

    html.H1("Dataframe C"),
    dash_table.DataTable(
        id='table-dfc',
        columns=[{"name": i, "id": i} for i in dfc.columns],
        data=dfc.to_dict('records')
    )
])

# Run the app
if __name__ == '__main__':
    app.run_server(debug=True)
