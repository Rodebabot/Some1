import pandas as pd

# Assuming you have a DataFrame named df

# Define the values to drop and the column
values_to_drop = ['f', 'g', 'h', 'i']
column_to_drop_from = 'type'

# Drop rows containing the specified values in the specified column
df = df[~df[column_to_drop_from].isin(values_to_drop)]

print(df)
