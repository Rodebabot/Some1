import win32com.client

# Initialize Outlook application
outlook = win32com.client.Dispatch("Outlook.Application")

# Get the Inbox folder
inbox = outlook.GetNamespace("MAPI").GetDefaultFolder(6)

# Specify the target location to save attachments
save_location = r'C:\Your\Save\Location\\'

# Iterate through emails in the Inbox folder
for mail in inbox.Items:
    if mail.Subject == "what":
        # Save attachments to the specified location
        for attachment in mail.Attachments:
            attachment.SaveAsFile(save_location + attachment.FileName)

        # Move the email to a subfolder (replace "Your Subfolder Name")
        subfolder_name = "Your Subfolder Name"
        subfolder = inbox.Folders[subfolder_name]
        mail.Move(subfolder)
