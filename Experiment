import pandas as pd

# Sample DataFrame for illustration
data = {
    'Check': ['A-B', 'C-D', 'E-F', 'G-H'],
    'OtherColumn': [1, 2, 3, 4]
}

# Create the original DataFrame
df = pd.DataFrame(data)

# Split the 'Check' column into multiple columns
split_columns = df['Check'].str.split('-', expand=True)

# Assign new column names
split_columns.columns = ['Column1', 'Column2']

# Combine the new split columns with the original DataFrame (if needed)
df_split = pd.concat([df, split_columns], axis=1)

print(df_split)
