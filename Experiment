import pandas as pd

# Assuming dffp1 and dfp1 are your DataFrames

# Merge dffp1 with dfp1 on 'Gloss Reference' to fetch 'Obligor Org Rdmid'
merged_df = pd.merge(dffp1, dfp1[['Gloss Reference', 'Obligor Org Rdmid']], on='Gloss Reference', how='left')

# Display the merged DataFrame
print(merged_df)
