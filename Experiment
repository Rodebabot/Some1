import pandas as pd

# Sample DataFrame
data = {'col1': ['apple', 'banana', 'orange', 'apple', 'grapefruit']}
df = pd.DataFrame(data)

# Get the value counts
value_counts = df['col1'].value_counts(normalize=True)

# Convert to percentages and format with two decimal places
percentages = value_counts * 100
percentages = percentages.round(2)

# Print the results
print("Percentage Distribution of Unique Values:")
print(percentages)
