import win32com.client

# Initialize Outlook application
outlook = win32com.client.Dispatch("Outlook.Application")

# Get the Inbox folder
inbox = outlook.GetNamespace("MAPI").GetDefaultFolder(6)

# Iterate through emails in the Inbox folder
for i, mail in enumerate(inbox.Items):
    if i >= 50:
        break  # Stop after 50 iterations

    if mail.Subject == "whatever it is":
        # Save the attachment to a specific location
        attachments = mail.Attachments
        for attachment in attachments:
            attachment.SaveAsFile(r'C:\Your\Save\Location\\' + attachment.FileName)

        # Move the email to a subfolder
        subfolder = inbox.Folders["Your Subfolder Name"]
        mail.Move(subfolder)
